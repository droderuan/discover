{
  "npmScope": "discover",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api-atlas": {
      "tags": []
    },
    "api-genesis": {
      "tags": []
    },
    "api-nest": {
      "tags": []
    },
    "models-veritas": {
      "tags": []
    },
    "ui-andromeda": {
      "tags": []
    },
    "ui-ara": {
      "tags": []
    },
    "ui-next-auth": {
      "tags": []
    },
    "ui-next-config": {
      "tags": []
    },
    "ui/ara-e2e": {
      "tags": [],
      "implicitDependencies": ["ui-ara"]
    }
  }
}
