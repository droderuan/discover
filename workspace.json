{
  "version": 2,
  "projects": {
    "api-atlas": {
      "root": "apps/api/atlas",
      "sourceRoot": "apps/api/atlas/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api/atlas",
            "main": "apps/api/atlas/src/main.ts",
            "tsConfig": "apps/api/atlas/tsconfig.app.json",
            "assets": [
              "apps/api/atlas/src/assets",
              {
                "glob": "schema.prisma",
                "input": "libs/models/veritas/src/schema",
                "output": "."
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/atlas/src/environments/environment.ts",
                  "with": "apps/api/atlas/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-atlas:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/atlas/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api/atlas"],
          "options": {
            "jestConfig": "apps/api/atlas/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-genesis": {
      "root": "apps/api/genesis",
      "sourceRoot": "apps/api/genesis/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api/genesis",
            "main": "apps/api/genesis/src/main.ts",
            "tsConfig": "apps/api/genesis/tsconfig.app.json",
            "assets": [
              "apps/api/genesis/src/assets",
              {
                "glob": "schema.prisma",
                "input": "libs/models/veritas/src/schema",
                "output": "."
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/genesis/src/environments/environment.ts",
                  "with": "apps/api/genesis/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-genesis:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/genesis/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api/genesis"],
          "options": {
            "jestConfig": "apps/api/genesis/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "models-veritas": {
      "root": "libs/models/veritas",
      "sourceRoot": "libs/models/veritas/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/models/veritas/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/models/veritas"],
          "options": {
            "jestConfig": "libs/models/veritas/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-nest": {
      "root": "libs/shared/nest",
      "sourceRoot": "libs/shared/nest/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/nest/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/nest"],
          "options": {
            "jestConfig": "libs/shared/nest/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-andromeda": {
      "root": "libs/ui/andromeda",
      "sourceRoot": "libs/ui/andromeda/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/ui/andromeda/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/andromeda"],
          "options": {
            "jestConfig": "libs/ui/andromeda/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui/andromeda/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/components-andromeda",
            "config": {
              "configFolder": "libs/ui/andromeda/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-ara": {
      "root": "apps/ui/ara",
      "sourceRoot": "apps/ui/ara",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/ui/ara",
            "outputPath": "dist/apps/ui/ara"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "ui-ara:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "ui-ara:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "ui-ara:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ui/ara"],
          "options": {
            "jestConfig": "apps/ui/ara/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ui/ara/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "ui-react-config": {
      "root": "libs/ui/react/config",
      "sourceRoot": "libs/ui/react/config/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/ui/react/config/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/react/config"],
          "options": {
            "jestConfig": "libs/ui/react/config/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui/ara-e2e": {
      "root": "apps/ui/ara-e2e",
      "sourceRoot": "apps/ui/ara-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui/ara-e2e/cypress.json",
            "devServerTarget": "ui-ara:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ui-ara:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ui/ara-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "ui-ara"
}
