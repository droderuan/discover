{
  "cli": "nx",
  "id": "nest-module",
  "type": "object",
  "properties": {
    "app": {
      "type": "string",
      "description": "The name of app",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "module": {
      "type": "string",
      "description": "The name of module",
      "x-prompt": "Enter the name of the new module",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": ["app", "module"]
}
